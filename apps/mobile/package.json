{
  "name": "@momo-merchant/mobile",
  "version": "0.1.0",
  "description": "MoMo Merchant Companion App - React Native mobile application",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace ios/MoMoMerchant.xcworkspace -scheme MoMoMerchant -configuration Release -archivePath build/MoMoMerchant.xcarchive archive",
    "clean": "cd android && ./gradlew clean && cd ios && rm -rf build",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@nozbe/watermelondb": "^0.27.1",
    "@nozbe/sqlite": "^3.41.2",
    "@tanstack/react-query": "^4.35.3",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-hook-form": "^7.45.4",
    "react-native-background-fetch": "^4.2.1",
    "react-native-biometrics": "^3.0.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-get-random-values": "^1.9.0",
    "react-native-keychain": "^8.1.1",
    "react-native-mmkv": "^2.10.2",
    "react-native-permissions": "^3.10.1",
    "react-native-reanimated": "^3.4.2",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.24.0",
    "react-native-sms-retriever": "^1.1.1",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-vision-camera": "^3.6.15",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/runtime": "^7.22.9",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native/tsconfig.json": "^3.0.0",
    "@types/react": "^18.2.15",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-hook-form": "^7.45.0",
    "@types/uuid": "^9.0.4",
    "babel-jest": "^29.6.1",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "metro-react-native-babel-preset": "^0.76.8",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "private": true
}